############################################
#文件名称:makefile
#创建日期:2020/07/20
#作    者:樊忠祥 fzx
#文件功能:主要讲述了一些make编译的命令参数
############################################

.PHONY:make makeall clean cleanall help

###########################################
#help 伪目标命令
##########################################
help:
	@echo "******************************"
	@echo "make     :增量编译代码"
	@echo "make  -a :全量编译代码"
	@echo "clean    :增量删除中间文件"
	@echo "clean -a :全量删除中间文件"
	@echo "help     :打印帮助信息"
	@echo "******************************"

include makefile.include

